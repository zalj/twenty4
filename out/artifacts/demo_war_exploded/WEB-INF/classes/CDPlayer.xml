<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean class="soundsystem.BlankDisk" id="blankDisk">
        <constructor-arg value="Good Night"/>
        <constructor-arg value="Delos"/>
        <constructor-arg>
            <list>
                <value>Submarine</value>
                <value>Yellow</value>
                <value>Nobody cares you when you down and out</value>
                <value>Get Better</value>
            </list>
        </constructor-arg>
    </bean>

    <bean class="soundsystem.CDPlayer">
        <property name="compactDisc" ref="blankDisk"></property>
    </bean>
</beans>